@model List<WebProject_NetFramework.Models.TestScheduleDBModel>
@{
    ViewBag.Title = "TestSchedule";
    Layout = "~/Views/Shared/_StudentLayout.cshtml";
}

<div class="m-content">
    @foreach(var temp in Model)
