                                    <div class="m-invoice__item">
                                        <span class="m-invoice__subtitle">END DATE.</span>
                                        <span class="m-invoice__text">@temp.TestSchedule.EndTime.ToString("MM yyyy")</span>
                                        @*<span class="m-invoice__text">Aug 20, 2020</span>*@
                                    </div>
                                </div>
