    <script language="javascript"
            type="text/javascript">
    toastr.options = {
        "closeButton": false,
        "debug": false,
        "newestOnTop": true,
        "progressBar": true,
        "positionClass": "toast-top-center",
        "preventDuplicates": false,
        "onclick": null,
        "showDuration": "300",
        "hideDuration": "1000",
        "timeOut": "5000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
    };

    var viewData = @Html.Raw(new JavaScriptSerializer().Serialize(ViewData));
    var type = '@Html.Raw(ViewData["MessageType"])';
    console.log("View Data: ",viewData);
    var message = viewData[0];
    var type = viewData[1];
    if (message !== null && message !== undefined) {
        console.log(`Message IS: ${message["Value"]}`);
        console.log("Not Null Message Received");
        if (type != undefined && type["Value"] === "error")
            toastr.error(message["Value"]);
        else if(type != undefined && type["Value"] === "warning")
            toastr.warning(message["Value"]);
        else
            toastr.info(message["Value"]);

    }

    </script>

